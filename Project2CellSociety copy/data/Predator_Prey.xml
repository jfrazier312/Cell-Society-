<?xml version="1.0"?>
<simulation name="Predator_Prey" author="team19">
	<grid width="20" height="20"/>
	<neighborhood size="5">
		<edge type="constant" value="alive">
		</edge>
	</neighborhood>
	<states>
		<state color="#33cc33" reproductionTime="20">fish</state>
		<state color="#333333" reproductionTime="20" lifeTime="2">shark</state>
		<state color="#d9d9d9">empty</state>
	</states>
	<init default="empty" mode="param" shape="Rectangle">
	</init>
	<animation framesPerSec="1"></animation>
	<param name="percentEmpty" value="0.1"></param>
	<param name="percentShark" value="0.2"></param>
</simulation>
