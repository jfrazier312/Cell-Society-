<?xml version="1.0"?>
<simulation name="Segregation" author="team19">
	<grid width="20" height="20"/>
	<neighborhood size="5">
		<edge type="constant" value="alive">
		</edge>
	</neighborhood>
	<states>
		<state color="#666666">typeA</state>
		<state color="#333333">typeB</state>
		<state color="#d9d9d9">empty</state>
	</states>
	<init mode="enum" default="empty">
	</init>
	<animation framesPerSec="1"></animation>
	<param name="percentEmpty" value="0.3"></param>
	<param name="percentTypeA" value="0.5"></param>
	<param name="probability" value="0.3"></param>
</simulation>
