<?xml version="1.0"?>
<simulation name="Ant" author="team19">
	<grid width="20" height="20"/>
	<neighborhood size="5">
		<edge type="constant" value="empty">
		</edge>
	</neighborhood>
	<states>
		<state color="#00ff00">open</state>
		<state color="#000000">obstacle</state>
		<state color="#ff0000">home</state>
		<state color="#0000ff">food</state>
	</states>
	<init mode="enum" default="open" shape="Rectangle">
		<cell state="food" row="1" col="1"></cell>
	</init>
	<animation framesPerSec="1"></animation>
	<param name="percentObstacles" value="0.2"></param>
</simulation>