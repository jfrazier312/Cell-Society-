<?xml version="1.0"?>
<simulation name="Fire" author="team19">
	<grid width="20" height="20"/>
	<neighborhood size="5">
		<edge type="constant" value="empty">
		</edge>
	</neighborhood>
	<states>
		<state color="#00ff00">tree</state>
		<state color="#ff0000">burning</state>
		<state color="#000011">empty</state>
	</states>
	<init mode="enum" default="tree">
		<cell state="burning" row="10" col="10"></cell>
	</init>
	<animation framesPerSec="1"></animation>
	<param name="probability" value="1"></param>
</simulation>
